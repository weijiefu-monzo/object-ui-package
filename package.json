{
  "name": "@object-ui/components",
  "version": "1.0.0",
  "description": "A React/Preact UI component library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && npm run copy-styles",
    "copy-styles": "cp -r styles dist/ && rsync -R components/**/*.css dist/ && rsync -R components/**/*.d.ts dist/ && find dist -name '*.tsx' -delete",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "preact": ">=10",
    "react": ">=16"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "preact": ">=10",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "local"
  },
  "keywords": [
    "react",
    "preact",
    "ui",
    "components"
  ],
  "author": "",
  "license": "MIT"
}
