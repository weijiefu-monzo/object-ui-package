{
  "name": "@object-ui/components",
  "version": "1.0.0",
  "description": "A React/Preact UI component library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && npm run copy-styles",
    "copy-styles": "cp -r src/styles dist/ && rsync -R src/components/**/*.css dist/ && rsync -R src/components/**/*.d.ts dist/ && find dist -name '*.tsx' -delete",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "extract-llms": "npx @fluentui/storybook-llms-extractor --config llms.config.js"
  },
  "peerDependencies": {
    "preact": ">=10"
  },
  "dependencies": {
    "@fluentui/storybook-llms-extractor": "^0.0.2",
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/preact-vite": "^9.1.10",
    "preact": ">=10",
    "storybook": "^9.1.10",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "local"
  },
  "keywords": [
    "react",
    "preact",
    "ui",
    "components"
  ],
  "author": "",
  "license": "MIT"
}
